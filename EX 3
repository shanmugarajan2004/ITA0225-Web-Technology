<html>
    <style>
        .parent {
            position: relative;
            width: 300px;
            height: 300px;
            background-color: lightgray;
        }
        .child {
            position: relative;
            top: 50px;
            left: 50px;
            width: 100px;
            height: 100px;
            background-color: lightblue;
        }
    </style>
</head>
<body>
    <div class="parent">
        <div class="child">Child Element</div>
    </div>
